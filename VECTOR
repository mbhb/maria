#include <iostream>
#include <vector>
using namespace std;

int resolver(vector<int> v, int ini, int fin) {
	int mitad = (ini + fin) / 2;
	if (fin - ini == 0) {
		return v[ini];
	}
	else if (fin - ini == 1) {
		if (v[ini] > v[fin]) {
			return v[fin];
		}
		else {
			return v[ini];
		}
	}
	else if (v[mitad - 1] > v[mitad] && v[mitad] < v[mitad + 1]) {
		return v[mitad];
	}
	else if (v[mitad - 1] > v[mitad]) {
		return resolver(v, mitad + 1, fin);
	}
	else {
		return resolver(v, ini, mitad - 1);
	}
}

bool resuelveCaso() {
	if (!std::cin) // fin de la entrada
		return false;
	else {
		unsigned long long int n;
		int aux;
		vector<int> v;
		cin >> n;
		for (unsigned int i = 0; i < n; i++) {
			cin >> aux;
			v.push_back(aux);
		}
		cout << resolver(v, 0, v.size() - 1) << endl;
	}
	return true;
}
int main() {
	while (resuelveCaso());
	return 0;
}
