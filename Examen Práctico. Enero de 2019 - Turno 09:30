2.RECUERDA LA \N
  #include <sys/types.h>
  #include <sys/wait.h>
  #include <stdio.h>
  #include <stdlib.h>
  #include <unistd.h>
  
  int main(int argc, char *argv[]) {
      int pid = fork();
      if (pid == 0) {
          setsid();
          chdir("/tmp");
          printf("datos: %d %d %d %d\n", getpid(), getppid(), getpgid(getpid()), getsid(0));
          execvp(argv[1], &argv[1]);
      } else {
          waitpid(pid, NULL, 0);
          return 0;
      }
  }



